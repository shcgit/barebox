#!/bin/sh

mmc0.probe=1
mount /dev/mmc0.0 /mnt

unprotect /dev/nand0.MLO
erase /dev/nand0.MLO
cp /mnt/barebox-am33xx-myirtech-mlo.img /dev/nand0.MLO
protect /dev/nand0.MLO

unprotect /dev/nand0.boot
erase /dev/nand0.boot
cp /mnt/barebox-am33xx-myirtech.img /dev/nand0.boot
protect /dev/nand0.boot

umount /mnt

/env/bin/erase_env
