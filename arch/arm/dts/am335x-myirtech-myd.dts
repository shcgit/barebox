/dts-v1/;

#include <arm/am335x-myirtech-myd.dts>

/ {
	chosen {
		environment {
			compatible = "barebox,environment";
			device-path = &env_nand;
		};
	};

};

&nand_parts {
	env_nand: partition@180000 {
		label = "env";
		reg = <0x180000 0x40000>;
	};

	partition@1c0000 {
		label = "system";
		reg = <0x1c0000 0>;
	};
};

#include "mm_am335x.dtsi"
