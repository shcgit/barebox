#include <arm/imx51-digi-connectcore-som.dtsi>

#include <dt-bindings/input/input.h>
#include <dt-bindings/sound/fsl-imx-audmux.h>

/ {
	model = "Mega-Milas Informer i.MX51";
	compatible = "milas,informer-imx51",
		     "digi,connectcore-ccxmx51-som", "fsl,imx51";

	chosen {
		stdout-path = &uart1;

		environment@0 {
			compatible = "barebox,environment";
			device-path = &nfc, "partname:env";
		};
	};

	backlight {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "simple-bus";

/*		backlight@0 {
			compatible = "gpio-backlight";
			reg = <0>;
		};*/

		backlight@1 {
			compatible = "gpio-backlight";
			reg = <1>;
			gpios = <&outgpios 4 GPIO_ACTIVE_HIGH>;
		};
	};

	beeper: beeper {
		compatible = "gpio-beeper";
		gpios = <&gpio3 23 GPIO_ACTIVE_HIGH>;
	};

	clocks {
		clk20m: clk20m {
			compatible = "fixed-clock";
			clock-frequency = <20000000>;
			#clock-cells = <0>;
		};

		clk54m: clk54m {
			compatible = "fixed-clock";
			clock-frequency = <54000000>;
			#clock-cells = <0>;
		};
	};

	csi@csi0 {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu_csi0>;
	};

	csi@csi1 {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu_csi1>;
	};

	display@di0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "fsl,imx-parallel-display";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu_disp0>;
		interface-pix-fmt = "rgb565";

		display-timings {
			native-mode = <&timing_di0_0>;

			timing_di0_0: PH320240T {
				clock-frequency = <6394000>;
				hactive = <320>;
				vactive = <240>;
				hback-porch = <38>;
				hfront-porch = <20>;
				hsync-len = <30>;
				vback-porch = <15>;
				vfront-porch = <5>;
				vsync-len = <3>;
				pixelclk-active = <1>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <1>;
			};
		};

		port@0 {
			reg = <0>;

			disp0_in: endpoint {
				remote-endpoint = <&ipu_di0_disp1>;
			};
		};
	};

/*
	display@di1 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "fsl,imx-parallel-display";
		pinctrl-names = "default";
