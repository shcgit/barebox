	gpiokeys: gpiokeys {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpiokeys>;
		compatible = "gpio-keys";

		button@0 {
			label = "CONFIRM";
			linux,code = <SW_MICROPHONE_INSERT>;
			linux,input-type = <EV_SW>;
			gpios = <&gpio3 22 GPIO_ACTIVE_LOW>;
		};

		button@1 {
			label = "PF_LOW";
			linux,code = <SW_TABLET_MODE>;
			linux,input-type = <EV_SW>;
			debounce-interval = <0>;
			gpios = <&gpio3 19 GPIO_ACTIVE_LOW>;
		};

		button@2 {
			label = "PF_HIGH";
			linux,code = <SW_HEADPHONE_INSERT>;
			linux,input-type = <EV_SW>;
			debounce-interval = <0>;
			gpios = <&gpio3 18 GPIO_ACTIVE_LOW>;
		};

		button@3 {
			label = "REVERSE";
			linux,code = <SW_RFKILL_ALL>;
			linux,input-type = <EV_SW>;
			gpios = <&gpio3 17 GPIO_ACTIVE_LOW>;
		};

		button@4 {
			label = "RESERVED1";
			linux,code = <SW_DOCK>;
			linux,input-type = <EV_SW>;
			gpios = <&gpio3 20 GPIO_ACTIVE_LOW>;
		};

		button@5 {
			label = "RESERVED2";
			linux,code = <SW_LINEOUT_INSERT>;
			linux,input-type = <EV_SW>;
			gpios = <&gpio3 21 GPIO_ACTIVE_LOW>;
		};

		button@6 {
			label = "RESERVED3";
			linux,code = <SW_JACK_PHYSICAL_INSERT>;
			linux,input-type = <EV_SW>;
			gpios = <&gpio3 9 GPIO_ACTIVE_LOW>;
		};
	};

	otgid: extcon {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_otgid>;
		compatible = "linux,extcon-usb-gpio";
		id-gpio = <&gpio4 19 GPIO_ACTIVE_HIGH>;
	};

	reg_video3v3: regulator@0 {
		compatible = "regulator-fixed";
		regulator-name = "video3v3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&outgpios 1 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	usbphy1: usbphy1 {
		compatible = "usb-nop-xceiv";
		clocks = <&clks IMX5_CLK_USB_PHY_GATE>;
		clock-names = "main_clk";
		#phy-cells = <0>;
	};

	soc {
		aips@80000000 {
			vpu: vpu@83ff4000 {
				compatible = "fsl,imx51-vpu";
				reg = <0x83ff4000 0x1000>;
				interrupts = <9>;
				clocks = <&clks IMX5_CLK_VPU_REFERENCE_GATE>,
					 <&clks IMX5_CLK_VPU_GATE>;
				clock-names = "per", "ahb";
				resets = <&src 1>;
				iram = <&iram>;
			};
		};
	};

	sound {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "simple-bus";

		audio@0 {
			reg = <0>;
			compatible = "simple-audio-card";
			simple-audio-card,name = "SSI0WM8753";
			simple-audio-card,format = "i2s";
			simple-audio-card,bitclock-master = <&audio0_master>;
			simple-audio-card,frame-master = <&audio0_master>;
			simple-audio-card,widgets =
				"Headphone", "Line Out";
			simple-audio-card,routing =
				"Line Out", "LOUT1",
				"Line Out", "ROUT1";

			simple-audio-card,cpu {
				sound-dai = <&ssi1>;
			};

			audio0_master: simple-audio-card,codec {
				sound-dai = <&wm8753>;
				system-clock-frequency = <12000000>;
			};
		};

		audio@1 {
			reg = <1>;
			compatible = "simple-audio-card";
			simple-audio-card,name = "SSI1TW2837";
			simple-audio-card,format = "i2s";
			simple-audio-card,bitclock-master = <&audio1_master>;
			simple-audio-card,frame-master = <&audio1_master>;
			/*simple-audio-card,widgets =
				"Headphone", "Line Out";
			simple-audio-card,routing =
				"Line Out", "LOUT1",
				"Line Out", "ROUT1";*/

			audio1_master: simple-audio-card,cpu {
				sound-dai = <&ssi2>;
			};

			simple-audio-card,codec {
				sound-dai = <&tw2837_audio>;
				/*system-clock-frequency = <12000000>;*/
			};
		};
	};
};

&audmux {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_audmux>;
	status = "okay";

	audmux-ssi0 {
		fsl,audmux-port = <MX51_AUDMUX_PORT1_SSI0>;
		fsl,port-config = <
			(IMX_AUDMUX_V2_PTCR_TFSDIR |
			IMX_AUDMUX_V2_PTCR_TFSEL(MX51_AUDMUX_PORT3) |
			IMX_AUDMUX_V2_PTCR_TCLKDIR |
			IMX_AUDMUX_V2_PTCR_TCSEL(MX51_AUDMUX_PORT3) |
			IMX_AUDMUX_V2_PTCR_SYN)
			IMX_AUDMUX_V2_PDCR_RXDSEL(MX51_AUDMUX_PORT3)
		>;
	};

	audmux-aud3 {
		fsl,audmux-port = <MX51_AUDMUX_PORT3>;
		fsl,port-config = <
			IMX_AUDMUX_V2_PTCR_SYN
			IMX_AUDMUX_V2_PDCR_RXDSEL(MX51_AUDMUX_PORT1_SSI0)
		>;
	};

	audmux-ssi1 {
		fsl,audmux-port = <MX51_AUDMUX_PORT2_SSI1>;
		fsl,port-config = <
			(IMX_AUDMUX_V2_PTCR_TFSDIR |
			IMX_AUDMUX_V2_PTCR_TFSEL(MX51_AUDMUX_PORT5) |
			IMX_AUDMUX_V2_PTCR_TCLKDIR |
			IMX_AUDMUX_V2_PTCR_TCSEL(MX51_AUDMUX_PORT5) |
			IMX_AUDMUX_V2_PTCR_SYN)
			IMX_AUDMUX_V2_PDCR_RXDSEL(MX51_AUDMUX_PORT5)
		>;
	};

	audmux-aud5 {
		fsl,audmux-port = <MX51_AUDMUX_PORT5>;
		fsl,port-config = <
			IMX_AUDMUX_V2_PTCR_SYN
			IMX_AUDMUX_V2_PDCR_RXDSEL(MX51_AUDMUX_PORT2_SSI1)
		>;
	};
};

&ecspi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1 &pinctrl_ecspi1cs>;
	fsl,spi-num-chipselects = <3>;
	cs-gpios = <&gpio4 24 GPIO_ACTIVE_HIGH>, <&gpio3 3 GPIO_ACTIVE_LOW>,
		   <&gpio3 13 GPIO_ACTIVE_LOW>;

	relgpios: gpio@1 {
		compatible = "fairchild,74hc595";
		reg = <1>;
		spi-max-frequency = <4000000>;
		registers-number = <1>;
		gpio-controller;
		#gpio-cells = <2>;
	};

	outgpios: gpio@2 {
		compatible = "nxp,74lvc594";
		reg = <2>;
		spi-max-frequency = <4000000>;
		registers-number = <1>;
		gpio-controller;
		#gpio-cells = <2>;
	};
};

&ecspi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi2>;
	fsl,spi-num-chipselects = <4>;
	cs-gpios = <&gpio3 6 GPIO_ACTIVE_LOW>, <&gpio1 0 GPIO_ACTIVE_LOW>,
		   <&gpio1 1 GPIO_ACTIVE_LOW>, <&gpio4 26 GPIO_ACTIVE_LOW>;
	status = "okay";

	max14830: max14830@0 {
		compatible = "maxim,max14830";
		reg = <0>;
		spi-max-frequency = <16000000>;
		clocks = <&clk20m>;
		clock-names = "osc";
		interrupt-parent = <&gpio3>;
		interrupts = <7 IRQ_TYPE_EDGE_FALLING>;
		gpio-controller;
		#gpio-cells = <2>;
	};

	mcp2515@1 {
		compatible = "microchip,mcp2515";
		reg = <1>;
		spi-max-frequency = <10000000>;
		clocks = <&clk20m>;
		interrupt-parent = <&gpio4>;
		interrupts = <13 IRQ_TYPE_EDGE_FALLING>;
	};

	mcp2515@2 {
		compatible = "microchip,mcp2515";
		reg = <2>;
		spi-max-frequency = <10000000>;
		clocks = <&clk20m>;
		interrupt-parent = <&gpio4>;
		interrupts = <14 IRQ_TYPE_EDGE_FALLING>;
	};

	mcp2515@3 {
		compatible = "microchip,mcp2515";
		reg = <3>;
		spi-max-frequency = <10000000>;
		clocks = <&clk20m>;
		interrupt-parent = <&gpio4>;
		interrupts = <25 IRQ_TYPE_EDGE_FALLING>;
	};
};

&esdhc1 {
	non-removable;
	status = "okay";
};

&gpio3 {
	ioen {
		gpio-hog;
		gpios = <2 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "IO ENABLE";
	};

	usb1rst {
		gpio-hog;
		gpios = <8 GPIO_ACTIVE_HIGH>;
		output-high;
		line-name = "USB1 RESET";
	};
};

&gpio4 {
	disp1gpio {
		gpio-hog;
		gpios = <31 GPIO_ACTIVE_LOW>;
		input;
		line-name = "DISP1 GPIO";
	};
};

&i2c2 {
	/* WM8753 SCLK Low pulse-width MIN 1.3us */
	clock-frequency = <350000>;

	wm8753: wm8753@1b {
		#sound-dai-cells = <0>;
		compatible = "wlf,wm8753";
		reg = <0x1b>;
	};

	isl97671a@2c {
		compatible = "intersil,isl97671a";
		reg = <0x2c>;
	};

	tw2837: tw2837@42 {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_tw2837>;
		compatible = "intersil,tw2837";
		reg = <0x42>;
		clocks = <&clk54m>;
		interrupt-parent = <&gpio3>;
		interrupts = <14 IRQ_TYPE_LEVEL_HIGH>;
		vddo-supply = <&reg_video3v3>;
		reset-gpios = <&outgpios 0 GPIO_ACTIVE_HIGH>;

		tw2837_audio: audio {
			#sound-dai-cells = <0>;
			/**/
		};

		video {
		};
	};

	at24@50 {
		compatible = "at,24c16";
		reg = <0x50>;
		pagesize = <256>;
	};

	rtc@68 {
		compatible = "maxim,ds3231";
		reg = <0x68>;
		#clock-cells = <1>;
	};
};

&iim {
	barebox,provide-mac-address = <&fec 1 9>;
};

&ipu {
	ipu_csi0: port@0 {
		reg = <0>;
	};

	ipu_csi1: port@1 {
		reg = <1>;
	};
};

&ipu_di0_disp1 {
	remote-endpoint = <&disp0_in>;
};

